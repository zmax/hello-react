{
  "name": "hello-react",
  "version": "1.0.0",
  "description": "Hello React with webpack, a starter project.",
  "main": "src/app.js",
  "scripts": {
    "build": "webpack --progress --colors --config webpack.config.prod.js",
    "dev": "webpack-dev-server --open --progress --colors --port 8888",
    "test": "jest --verbose",
    "testnc": "jest --no-cache"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/test/jest-preprocessor",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/classnames",
      "<rootDir>/node_modules/enzyme",
      "<rootDir>/node_modules/sinon",
      "<rootDir>/node_modules/redux",
      "<rootDir>/node_modules/react-redux",
      "<rootDir>/node_modules/react-router-redux"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zmax/hello-react.git"
  },
  "keywords": [
    "reactjs",
    "react",
    "starter",
    "webpack",
    "babel",
    "es2015"
  ],
  "author": "Starck Lin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zmax/hello-react/issues"
  },
  "homepage": "https://github.com/zmax/hello-react#readme",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "jquery": "^1.12.3"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-jest": "^10.0.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jest-cli": "^0.10.2",
    "node-sass": "^3.4.2",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.2",
    "redux": "^3.5.1",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "whatwg-fetch": "^0.11.0"
  }
}
