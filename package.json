{
  "name": "hello-react",
  "version": "1.0.0",
  "description": "Hello React with webpack, a starter project.",
  "main": "src/app.js",
  "scripts": {
    "build": "webpack --progress --colors --config webpack.config.prod.js",
    "dev": "webpack-dev-server --open --progress --colors --port 8888",
    "test": "jest --verbose"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zmax/hello-react.git"
  },
  "keywords": [
    "reactjs",
    "react",
    "starter",
    "webpack",
    "babel",
    "es2015"
  ],
  "author": "Starck Lin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zmax/hello-react/issues"
  },
  "homepage": "https://github.com/zmax/hello-react#readme",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "jquery": "^1.11.3",
    "react": "^15.0.0",
    "classnames": "^2.2.3"
  },
  "devDependencies": {
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1",
    "babel-core": "^6.7.5",
    "babel-jest": "^10.0.1",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "jest-cli": "^0.10.0",
    "react-dom": "^15.0.0",
    "react-addons-test-utils": "^15.0.0"
  }
}
